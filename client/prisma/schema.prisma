generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Packages {
  id         Int    @id @default(autoincrement())
  idPackages Int    @unique
  status     String
  user       String
  addressIn  String
  addressOut String
}
